{
  "{{chrome}}.manifest_version": 3,
  "{{firefox}}.manifest_version": 2,
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "96": "icon/96.png",
    "128": "icon/128.png"
  },
  "default_locale": "en",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "src/scripts/background.ts"
  },
  "{{chrome}}.action": {
    "default_popup": "src/popup/index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
